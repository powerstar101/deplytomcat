pipeline{
    agent any
    stages{
        stage("dowload artifacts"){
            steps{
                println "here we download artifacts from s3 into local"
                sh """
                echo 'Pulling...' + env.BRANCH_NAME
                echo $BUILD_NUMBER
                 aws s3 ls
                 aws s3 cp s3://jenkin101/$BRANCH_NAME/$BUILD_NUMBER/hello-*.war/
                 ls -l
                 
                """
            }
        }
        stage("deploy artifacts"){
            steps{
                println "here we deploy artifacts"
            }
        }

    }
}