pipeline {
    agent any
    stages{
        stage("deploy"){
            steps{
                script {
                    println "here we are doing deploy"
                    sh '''
                    #!bin/bash/
                    echo $branch
                    echo $build_number
                    echo $serverip

                    myservers=$@

                    IFS="," read -r -a outputservers <<< $myservers

                    for serverip in ${outputservers[@]}
                    do 
                            aws s3 cp s3deply101/origin/$branch/$build_number/hello-$build_number.war .

                    done
                    '''
                }
            }
        }
    }
}